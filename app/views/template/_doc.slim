section(title="Identification")
  question(text="Name of Claimant:")
    TextBox(name="Q1_FIRST" placeholder="First")
    TextBox(name="Q1_LAST" placeholder="Last")
  question(text="Date of Birth:")
    TextBox(name="Q1_BIRTHDATE" placeholder="dd/mm/yyyy")
  question(text="Sex:")
    RadioSet(name="gender")
        Value Male
        Value Female
  question(text="Case No:")
    TextBox(name="Q1_CASENUM")
  question(text="RQID: ")
    TextBox(name="Q1_RQID")
  question(text="Adjudicator:")
    TextBox(name="Q1_ADJUDICATOR")
  question(text="Date and Time of Exam: (dd/mm/yyyy)")
    TextBox(name="Q1_EXAMDATE" placeholder="dd/mm/yyyy")
  question(text="Location:")
    TextBox(name="Q1_EXAMLOC")
  question(text="Examination type:")
    RadioSet(name="Q3")
      Value(enables-selector=".enable-child") Child Psychological Diagnostic Evaluation (turn on different questions to be determined) 
      Value(enables-selector=".enable-complex") Complex Psychological Evaluation
      Value(enables-selector=".enable-psych-only") Psychological Diagnostic Evaluation
      Value(enables-selector=".enable-psych-mem") Psychological Diagnostic Evaluation with Memory Assessment
      Value(enables-selector=".enable-psych-intel") Psychological Diagnostic Evaluation with Intelligence Assessment


  question(name="Q5" text="The claimant's ethnicity/race is:")
    CheckboxSet
      Value American Indian
      Value African American
      Value Asian American
      Value Caucasian American
      Value Hispanic American
      Value Native Hawaiian
      Value Pacific Islander
      ValueOther Other
  question(name="Q6" text="What is the claimant’s birthplace?")
    TextBox


section#General-Observations
  section_text General Observations
  question(name="Q8" text="The claimant arrived to the examination:")
    RadioSet
      Value Early
      Value On time
      Value Late
  question(name="Q8A" text="The claimant was:")
    RadioSet
      Value Unaccompanied
      ValueOther.prepend Accompanied by
  question(name="q9" text="How did the claimant get to the examination?")
    RadioSet
      Value(value="personal vehicle") Drove
      Value(value="transportation arranged by DSHS") DSHS arranged transportation
      Value Taxi
      Value Public transportation
      ValueOther.prepend Driven by
  question(name="Q11" text="Is the claimant considered a reliable historian?")
    RadioSet
      Value(value="is considered") Yes
      Value(value="is not considered" enables-selector=".11_no") No
  question.11_no(name="Q11A" text="Who is the reliable historian for this interview?")
    TextBox


section#Chief-Complaint
  section_text Chief Complaint
  question(name="Q13" text="What is the claimant’s chief complaint? Select all that apply.")
