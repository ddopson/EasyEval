    Value(value="is willing to consider psychotherapy") Yes
    Value(enables-selector=".20_no" value="is not willing to consider psychotherapy") No
question.19_yes(name="Q19A" text="With whom is the claimant currently in psychotherapy?")
  CheckboxSet
    Value(value=" with a counselor") A counselor
    Value(value=" with a psychologist") A psychologist
    Value(value=" with a therapist") A therapist
question.19_yes(name="Q20" text="Does the claimant find psychotherapy helpful?")
  RadioSet
    Value(value="has been") Yes
    Value(enables-selector=".20_no" value="has not been") No
question.20_no(name="Q20A" text="Please explain the claimant's opinion of psychotherapy:")
  TextBox

section#Current-Medications
  section_text Current Medications
  question(name="Q21" text="Is the claimant taking any medications?")
    RadioSet
      Value(enables-selector=".21_yes" value="is") Yes
      /enable 21a, 22, and 22table
      Value(value="is not") No
  group.21_yes
    question(name="Q177" text="Who is writing the prescriptions?")
      CheckboxSet
        Value A psychiatristf
        Value A PCP
        Value A nurse practitioner
    question(name="Q21A" text="Did the claimant take any medication today?")
      RadioSet
        Value(value="did take") Yes
        Value(value="did not take") No
    question(name="Q22" text="Which medications is the claimant taking?")
      table
        tr
          th Medication
          th Dosage
          th Effectiveness
        tr.table-row-model
          td
            Autocomplete(name="MEDICATION" values="meds")
          td
            TextBox(name="DOSAGE")
          td
            RadioSet(name="EFFECTIVENESS")
              Value Good
              Value Minimal
              Value Poor
        tr
          td(colspan=3 text-align="right")
            a.table-new-row New Row ...

section#Past-Psychiatric-History
  section_text Past Psychiatric History
  question(name="Q29" text="Has the claimant ever been admitted to a psychiatric hospital?")
    RadioSet
      Value(enables-selector=".29_yes" value="has") Yes
      / (enable 30,31,31a, and 31b)
      Value(enables-selector=".29_no" value="has never") No
      / (disable the rest of this section)
  group.29_yes
    question(name="Q30" text="What was the date of the last admittance? (Month Year)")
      TextBox
    question(name="Q31" text="What was the reason for the admittance?")
      TextBox
    question(name="Q31A" text="What treatment did the claimant receive while admitted?")
      TextBox
    question(name="Q31B" text="What was the claimant’s response to the treatment?")
      TextBox

section#Alcohol-and-Drug-Abuse
  section_text Alcohol and Drug Abuse
  question(name="QA32" text="Has the claimant ever abused alcohol, tobacco, or illicit substances?")
    RadioSet
      Value(enables-selector=".A32_yes" value="has abused") Yes
      / (enable A33-A36a, A189)
      Value(enables-selector=".A32_no" value="has not abused") No
      / (disable the rest of this section)
  group.A32_yes
    question(name="QA33" text="If yes, what are the substances? Select all that apply.")
      CheckboxSet
        Value Alcohol
        Value Marijuana
        Value Cocaine
        Value Heroin
        Value Methamphetamines
        Value Tobacco
        ValueOther Other
    question(name="QA34" text="On what date did claimant first use the substance? (Month Year)")
      TextBox
    question(name="QA34A" text="On what date did the claimant begin to use the substance abusively? (Month Year)")
      TextBox
    question(name="QA35" text=" What is the number of days that the claimant used the substance? (include unit of day, month, or year)")
      TextBox
    question(name="QA35A" text="During the claimant’s worst abusive period, how much of the substance was consumed per day?")
      TextBox
    question(name="QA36" text="Is the substance abuse still occurring?")
      RadioSet
        Value(value="is currently") Yes
        Value(enables-selector=".36_no" value="is no longer") No
        /enables a36a.
    question.36_no(name="QA36A" text="If not, how long has the claimant been ‘clean?’")
      TextBox
    question(name="QA189" text="Did the claimant appear to be under the influence of drugs or alcohol?")
      RadioSet
        Value(value="did appear") Yes
        Value(value="did not appear") No
    question(name="QA189B" text="List any substances abused by the claimant:")
      table
        tr
          th Substance
          th Start Date
          th End Date
          th Amount
          th Frequency
        tr.table-row-model
          td: TextBox(name="SUBSTANCE")
          td: TextBox(name="START" placeholder="mm/dd/yyyy")
          td: TextBox(name="END" placeholder="mm/dd/yyyy")
          td: TextBox(name="AMOUNT")
          td: TextBox(name="FREQUENCY")
        tr
          td(colspan=5 text-align="right")
            a.table-new-row New Row ...

