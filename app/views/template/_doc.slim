      Value(enables-selector="#Q16B") Anorexia
      Value(enables-selector="#Q16C") Anxiety
      Value(enables-selector="#Q16D") Attention Disorder
      Value(enables-selector="#Q16E") Autism Disorders
      Value(enables-selector="#Q16F") Bipolar Disorder
      Value(enables-selector="#Q16G") Cognitive Disorder NOS
      Value(enables-selector="#Q16H") Depression
      Value(enables-selector="#Q16I") Dysthymic Disorder
      Value(enables-selector="#Q16J") Learning Disability
      Value(enables-selector="#Q16K") Mental Retardation
      Value(enables-selector="#Q16L") Obsessive Compulsive Disorder
      Value(enables-selector="#Q16M") Panic Disorder
      Value(enables-selector="#Q16N") Personality Disorder
      Value(enables-selector="#Q16O") Postconcussion Syndrome
      Value(enables-selector="#Q16P") Posttraumatic Stress Disorder
