section#Review-of-Records
  section_text Review of Records
  question(name="QA14" text="Has the claimant been previously diagnosed?")
    RadioSet
      Value(enables-selector=".14_yes") Yes
      / (enable 14, 14a)
      Value(enables-selector=".14_no") No
      / (disable 14, 14a)
  question.14_yes(name="Q14" text="When was the claimant first diagnosed? (Month Year)")
    TextBox
  question.14_yes(name="Q14A" text="Who diagnosed the claimant?")
    TextBox
    question(name="Q14B" text="List any additional diagnoses previously made:")
      table
        tr
          th Clinician
          th Date
          th Diagnosis
        tr.table-row-model
          td
            TextBox(name="CLINICIAN")
          td
            TextBox(name="DATE")
          td
            TextBox(name="DIAGNOSIS")
        tr
          td(colspan=3 text-align="right")
            a.table-new-row New Row ...

  question(name="Q15" text="Were there any special circumstances surrounding the onset of this impairment or condition?")
    RadioSet
      Value(enables-selector=".15_yes") Yes
      Value No
  question.15_yes(name="Q15A" text="Please describe:")
    TextBox



section#History-of-Present-Illness
  section_text History of Present Illness
  question(name="Q16" text="What are the claimant’s current symptoms? Select all that apply.")
    = render partial: 'Q16-illness'
    question(name="Q16A" text="Please provide any additional symptoms:")
      TextBox


question(name="Q17" text="Describe in detail the effect of each impairment on the claimant’s daily functioning. Please focus on relationships and work ability.")
  TextBox
question(name="QA18" text="Please provide the date the claimant stopped working due to the inability to perform work duties:")
  TextBox
question(name="QA18A" text="Please provide any information regarding attempts to return to the workplace:")
  TextBox
question(name="Q19" text="Is the claimant currently in psychotherapy?")
  RadioSet
    Value(enables-selector=".19_yes" value="is") Yes
    /enable 19a, 20, and 20a. disable 179
    Value(enables-selector=".19_no" value="is not") No
    /enable 179. disable 19a, 20, 20a.
question.19_no(name="Q179" text="Is the claimant willing to consider psychotherapy?")
